# 编译psg1218a固件:
CONFIG_TARGET_ramips=y
CONFIG_TARGET_ramips_mt7620=y
CONFIG_TARGET_ramips_mt7620_DEVICE_phicomm_psg1218a=y
CONFIG_TESTING_KERNEL=y

CONFIG_DEFAULT_luci-app-accesscontrol=y
CONFIG_DEFAULT_luci-app-arpbind=y
CONFIG_DEFAULT_luci-app-autoreboot=y
CONFIG_DEFAULT_luci-app-ddns=n
CONFIG_DEFAULT_luci-app-filetransfer=n
CONFIG_DEFAULT_luci-app-nlbwmon=n
CONFIG_DEFAULT_luci-app-ssr-plus=n
CONFIG_DEFAULT_luci-app-turboacc=n
CONFIG_DEFAULT_luci-app-upnp=y
CONFIG_DEFAULT_luci-app-vlmcsd=n
CONFIG_DEFAULT_luci-app-vsftpd=n
CONFIG_DEFAULT_luci-app-wol=n
# CONFIG_PACKAGE_luci-app-passwall is not set
# CONFIG_PACKAGE_luci-app-passwall2 is not set
# CONFIG_PACKAGE_luci-app-bypass is not set
# CONFIG_PACKAGE_luci-app-vssr is not set

# 固件压缩:
#CONFIG_TARGET_IMAGES_GZIP=y

#Images
CONFIG_TARGET_ROOTFS_INITRAMFS=y
CONFIG_TARGET_INITRAMFS_COMPRESSION_NONE=y
CONFIG_EXTERNAL_CPIO=""
CONFIG_TARGET_ROOTFS_SQUASHFS=y
CONFIG_TARGET_SQUASHFS_BLOCK_SIZE=1024
CONFIG_TARGET_UBIFS_FREE_SPACE_FIXUP=y
CONFIG_TARGET_UBIFS_JOURNAL_SIZE=""
CONFIG_DOWNLOAD_CHECK_CERTIFICATE=y
# CONFIG_TARGET_ROOTFS_EXT4FS is not set


# IPv6支持:
CONFIG_PACKAGE_dnsmasq_full_dhcpv6=y
CONFIG_PACKAGE_ip6tables=y
CONFIG_PACKAGE_ipv6helper=y

